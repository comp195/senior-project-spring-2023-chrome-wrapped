{
  "manifest_version": 3,
  "name": "Chrome Wrapped",

  "description": "A Chrome Extension to improve browsing history experience",
  "version": "1.0.2",
  "action": {
    "default_popup": "index.html",
  "default_title": "Open",
    "default_icon":"./chromeIcon128.png"
  },
  "icons":{
    "128":"./Icon 128.png",
    "48":"./Icon 48.png",
    "16":"./Icon 16.png"

  },
  "permissions": [
    "history"
  ],
  //Chrome doesn't allow this setting, but other browsers require it. Change manifest.json to this file when using firefox and/or other browsers.
  "browser_specific_settings": {
    "gecko": {
      "id": "chromewrapped@pacific.edu",
      "strict_min_version": "42.0"
    }
  }
  
}