{
  "manifest_version": 3,
  "name": "Chrome Wrapped",
  "description": "A Chrome Extension to improve browsing history experience",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html",
  "default_title": "Open",
  "browser_action":{
    "default_icon":"./chromeIcon128.png"
  }
  },
  "icons":{
    "16":"./chromeIcon16.png",
    "48":"./chromeIcon48.png",
    "128":"./chromeIcon128.png"
  },
  "permissions": [
    "scripting"
  ]
}